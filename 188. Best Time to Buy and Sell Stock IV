不是很懂这个dp算法
class Solution {
public:
    int maxProfit(int k, vector<int>& prices) {
        int maxProfit=0;
        
        if(prices.size()<2)
            return 0;
        if(k>prices.size()/2){
            for(int i=1; i<prices.size(); i++)
                maxProfit += max(prices[i]-prices[i-1], 0);
            return maxProfit;
        }
        
        int hold[k+1];
        int rele[k+1];
        for (int i=0;i<=k;++i){
            hold[i] = INT_MAX;
            rele[i] = 0;
        }
        
        for(int i=0; i<prices.size(); i++){
            for(int j=k; j>=1; j--){
                rele[j] = max(rele[j], prices[i]-hold[j]);//利润
                hold[j] = min(hold[j], prices[i]-rele[j-1]);//持有资产
                maxProfit=max(maxProfit, rele[j]);
            }
        }
        return maxProfit;
    }
};
